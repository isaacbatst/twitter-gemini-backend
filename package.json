{
  "name": "twiter-gemini-backend",
  "version": "1.0.0",
  "main": "src/server.js",
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "nodemon ./src/server.js --watch src",
    "sequelize": "sequelize",
    "migrate": "sequelize db:migrate",
    "migrate:reset": "sequelize db:migrate:undo:all && npm run migrate",
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset"
  },
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "fastexpress": "latest",
    "mysql2": "^1.6.1",
    "sequelize": "5.15.1",
    "sequelize-cli": "^5.2.0"
  },
  "devDependencies": {
    "cross-env": "^6.0.3",
    "dotenv": "^8.2.0",
    "jest": "^24.9.0",
    "nodemon": "^1.18.5",
    "supertest": "^4.0.2"
  }
}
